  "scripts": {
    "dev": "nodemon  -w ./ --exec \"babel-node app.js --presets es2015,stage-0\" --inspect",
    "start": "node  -w ./ --exec \"babel-node app.js --presets es2015,stage-0\" --inspect"
  },
  "author": "Daily Code Buffer",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "debug": "4.1.1",
    "express": "^4.17.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },

  "devDependencies": {
    "nodemon": "^2.0.15",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
